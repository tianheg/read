{
  "name": "read",
  "devDependencies": {
    "eslint": "^8.52.0",
    "husky": "latest",
    "lint-staged": "^15.0.2",
    "vitepress": "latest"
  },
  "lint-staged": {
    ".vitepress/**/*.js": "eslint --fix --ignore-pattern '!.vitepress'",
    "scripts/*.mjs": "eslint --fix --ignore-pattern '!scripts'"
  },
  "private": true,
  "scripts": {
    "dev": "vitepress dev --port 4321",
    "build": "vitepress build",
    "lint": "eslint .vitepress/**/*.js --fix --ignore-pattern '!.vitepress' && eslint scripts/*.mjs --fix --ignore-pattern '!scripts'",
    "prepare": "husky"
  },
  "type": "module"
}
